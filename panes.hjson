{
  verbs:
  [
    {
      key: f3
      internal: ":toggle_second_tree"
    }
    {
      key: f4
      shortcut: z
      external: tmux splitw
      set_working_dir: true
      leave_broot: false
    }
    {
      key: ctrl-t
      shortcut: tab
      external: tmux new-window -c {directory}
      set_working_dir: true
      leave_broot: false
    }
  ]
}
